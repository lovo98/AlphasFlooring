{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"~assets/*": [
				"src/assets/*"
			],
			"~components/*": [
				"src/components/*"
			],
			"~Routers/*": [
				"src/Routers/*"
			],
			"~/*": [
				"src/*"
			],
			"~Services/*": [
				"src/components/Services/*"
			],
			"~ServicesPage/*": [
				"src/components/ServicesPage/*"
			],
			"~helpers/*": [
				"src/helpers/*"
			],
			"~config/*": [
				"src/config/*"
			],
			"~Login/*": [
				"src/components/Login/*"
			],
			"~context/*": [
				"src/context/*"
			],
			"~hooks/*": [
				"src/hooks/*"
			]
		}
	}
}