{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"~assets/*": [
				"src/assets/*"
			],
			"~components/*": [
				"src/components/*"
			],
			"~Routers/*": [
				"src/Routers/*"
			],
			"~/*": [
				"src/*"
			],
			"~AboutUs/*": [
